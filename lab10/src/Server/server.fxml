<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<GridPane fx:controller="Server.ServerController"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">

    <TextArea fx:id="textArea" editable="false" GridPane.rowIndex="0" prefWidth="275" prefHeight="200"></TextArea>
    <Button text="Exit" onAction="#exit" GridPane.rowIndex="1" GridPane.columnIndex="0"></Button>
</GridPane>
